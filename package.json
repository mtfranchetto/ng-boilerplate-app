{
    "name": "ng-boilerplate-app",
    "version": "0.3.0",
    "description": "Boilerplate to start AngularJS applications",
    "repository": {
        "type": "git",
        "url": "https://github.com/mtfranchetto/ng-boilerplate-app"
    },
    "dependencies": {
        "jquery": "^2.1.1",
        "lodash": "^2.4.1",
        "ng-common": "^0.5.0",
        "normalize-compass": "^3.0.1"
    },
    "devDependencies": {
        "browserify": "^5.5.0",
        "browserify-istanbul": "^0.1.2",
        "browserify-shim": "^3.7.0",
        "envify": "^3.0.0",
        "gulp": "^3.8.7",
        "smild": "^0.1.3"
    },
    "engines": {
        "node": ">=0.10.0"
    },
    "browser": {
        "angular": "./bower_components/angular/angular.js",
        "angular-route": "./bower_components/angular-route/angular-route.js"
    },
    "browserify": {
        "transform": [
            "browserify-shim",
            "envify"
        ]
    },
    "browserify-shim": {
        "angular": {
            "depends": ["jquery:jQuery"],
            "exports": "angular"
        }
    },
    "license": "MIT",
    "scripts": {
        "install": "bower install"
    },
    "smild": {
        "bundleNoParse": [
            "../lodash",
            "../jquery"
        ]
    }
}
